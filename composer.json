{
    "name": "inkdez/app-manager",
    "description": "App manager for dynamic Collection on Inkdez sibcdc app",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Inkdez\\AppManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Inkdez\\AppManager\\InkdezAppServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Nelson Alexandre Mutane",
            "email": "inkomomutane@github.com"
        }
    ],

    "minimum-stability": "dev",
    "require": {
        "illuminate/support": "9.x",
        "illuminate/filesystem": "9.x",
        "jenssegers/mongodb": "^3.9"
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.x-dev",
        "thedoctor0/laravel-factory-generator": "dev-master"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
